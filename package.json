{
  "name": "gorillas",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npx ts-node --esm build.ts",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start": "node ./dist/index.js --env=production",
    "dev": "nodemon",
    "runGorillas": "ts-node --esm runGorillas.ts",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --esm --files -r tsconfig-paths/register ./src/index.ts"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "exports": null,
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^7.0.6",
    "express-ws": "^5.0.2",
    "helmet": "^6.1.5",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^1.3.1",
    "jet-validator": "^1.0.4",
    "jimp": "^0.22.7",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "keycode": "^2.2.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "ref-struct-napi": "^1.1.1",
    "ref-union-napi": "^1.0.2",
    "ts-command-line-args": "^2.4.2",
    "win32-api": "^20.4.1",
    "windows-ffi": "^0.0.4",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.1",
    "@types/ffi-napi": "^4.0.7",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.1",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.1",
    "jasmine": "^4.6.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
